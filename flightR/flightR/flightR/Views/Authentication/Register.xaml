<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="flightR.Views.Authentication.Register">
    <ContentPage.Content>
        <StackLayout Spacing="20" Padding="50" VerticalOptions="Center">
            <Image Source="flightr.png" VerticalOptions="FillAndExpand"></Image>
            <Entry x:Name="entryUsername" Placeholder="Username"></Entry>
            <Entry x:Name="entryPassword" Placeholder="Password" IsPassword="True"></Entry>
            <Button x:Name="buttonRegister" Text="Log In" TextColor="White" BackgroundColor="##ff77D065" Clicked="gotMain"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>